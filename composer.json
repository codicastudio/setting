{
    "name": "codicastudio/setting",
    "description": "A random Codica Studio package.",
    "homepage": "https://github.com/codicastudio/setting",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
    "psr-4": {
            "codicastudio\\Setting\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Setting\\Provider"
            ],
            "aliases": {
                "Setting": "codicastudio\\Setting\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
